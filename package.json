{
    "name": "vscode-util",
    "displayName": "vscode-util",
    "description": "This is utility tool",
    "version": "0.0.1",
    "publisher": "georgeguo",
    "engines": {
        "vscode": "^1.4.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:util.sort"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "util.sort",
            "title": "Sort Asc"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}